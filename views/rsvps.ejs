<% include header %>

	<body>
		<% include navigation %>
		<div class="col-sm-9 col-sm-offset-3 col-lg-10 col-lg-offset-2 main">			
			<div class="row">
				<ol class="breadcrumb">
					<li><a href="#"><svg class="glyph stroked home"><use xlink:href="#stroked-home"></use></svg></a></li>
					<li class="active">RSVPs</li>
				</ol>
			</div><!--/.row-->
			
			<div class="row">
				<div class="col-lg-12">
					<h1 class="page-header">RSVPs</h1>
				</div>
			</div><!--/.row-->

				<%  var i = 0; %>
				<% 	data.results.forEach(function(rsvp) { %>

				
					<%	if (rsvp.attending) { %>
						<% 	include widgets/attendingRsvp %>
					<% 	} else { %>
				   		<% include widgets/notattendingRsvp %>
				   	<% 	} %>

					<% 	i++; %>
				<% 	}); %>
			
		</div>	<!--/.main-->
		
	</body>
</html>
